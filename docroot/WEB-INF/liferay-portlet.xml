<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>LC-Survey</portlet-name>
		<icon>/icon.png</icon>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>lc</friendly-url-mapping>
		<friendly-url-routes>com/lc/survey/lc-friendly-url-routes.xml</friendly-url-routes> 
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-javascript>/js/jquery/jquery-1.11.2.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery/jquery-ui.js</header-portlet-javascript>
		<header-portlet-javascript>/js/components.js</header-portlet-javascript>
		<header-portlet-javascript>/js/instance.js</header-portlet-javascript>
		<header-portlet-javascript>/js/tips.js</header-portlet-javascript>
		<header-portlet-javascript>/js/charts.js</header-portlet-javascript>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>LC-Survey</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>